                                <Label layoutX="81.0" layoutY="14.0" text="Search Word" />
                                <CheckBox fx:id="FemaleCheckBox" layoutX="737.0" layoutY="43.0" mnemonicParsing="false" onAction="#showFemaleAction" text="Female" />
                                <CheckBox fx:id="MaleCheckBox" layoutX="676.0" layoutY="42.0" mnemonicParsing="false" onAction="#showMaleAction" text="Male" />
                            </children></AnchorPane>
                        <ScrollPane prefHeight="774.0" prefWidth="1214.0">
                            <content>
                                <TilePane fx:id="allCardContent" prefHeight="758.0" prefWidth="1198.0" />
                            </content>
                        </ScrollPane>
                    </children>
                </VBox>
            </content>
        </Tab>
