<TabPane prefHeight="906.0" prefWidth="1214.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.PrimaryController">
    <tabs>
        <Tab fx:id="allCardTab" text="All Cards">
            <content>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <AnchorPane prefHeight="101.0" prefWidth="865.0">
                            <children>
                                <TextField fx:id="searchedWord" layoutX="81.0" layoutY="39.0" prefHeight="26.0" prefWidth="509.0" />
