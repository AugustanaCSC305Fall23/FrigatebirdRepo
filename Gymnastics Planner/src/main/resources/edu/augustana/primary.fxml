                        <TilePane fx:id="selectedCardsView" prefHeight="725.0" prefWidth="605.0" />
                     </children>
                  </VBox>
               </children></AnchorPane>
            </content>
        </Tab>
        <Tab text="All Plans">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="169.0" prefWidth="343.0" />
            </content>
        </Tab>
