<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<TabPane prefHeight="600.0" prefWidth="865.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.PrimaryController">
    <tabs>
        <Tab fx:id="allCardTab" text="All Cards">
            <content>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <AnchorPane prefHeight="101.0" prefWidth="865.0">
                            <children>
                                <TextField fx:id="searchedWord" layoutX="81.0" layoutY="39.0" prefHeight="26.0" prefWidth="509.0" />
                                <Button fx:id="searchButton" layoutX="612.0" layoutY="39.0" mnemonicParsing="false" onAction="#searchButtonAction" text="Search" />
                                <Label layoutX="81.0" layoutY="14.0" text="Search Word" />
                                <CheckBox fx:id="FemaleCheckBox" layoutX="737.0" layoutY="43.0" mnemonicParsing="false" onAction="#showFemaleAction" text="Female" />
                                <CheckBox fx:id="MaleCheckBox" layoutX="676.0" layoutY="42.0" mnemonicParsing="false" onAction="#showMaleAction" text="Male" />
                            </children></AnchorPane>
                        <ScrollPane prefHeight="563.0" prefWidth="865.0">
                            <content>
                                <TilePane fx:id="allCardContent" prefHeight="543.0" prefWidth="847.0" />
                            </content>
                        </ScrollPane>
                    </children>
                </VBox>
            </content>
        </Tab>
        <Tab text="Create Plan">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
        </Tab>
        <Tab text="All Plans">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="169.0" prefWidth="343.0" />
            </content>
        </Tab>
        <Tab text="Add Cards">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
        </Tab>
        <Tab text="Exit">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
        </Tab>
    </tabs>
    <opaqueInsets>
        <Insets right="3.0" top="1.0" />
    </opaqueInsets>
</TabPane>

