<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="564.0" prefWidth="961.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.AllCardsController">
    <VBox prefHeight="562.0" prefWidth="960.0">
        <children>
            <AnchorPane prefHeight="101.0" prefWidth="865.0">
                <children>
                    <TextField fx:id="searchedWord" layoutX="81.0" layoutY="39.0" prefHeight="26.0" prefWidth="509.0" />
                    <Button fx:id="searchButton" layoutX="612.0" layoutY="39.0" mnemonicParsing="false" onAction="#searchButtonAction" text="Search" />
                    <Label layoutX="81.0" layoutY="14.0" text="Search Word" />
                    <CheckBox fx:id="femaleCheckBox" layoutX="737.0" layoutY="43.0" mnemonicParsing="false" onAction="#showFemaleAction" text="Female" />
                    <CheckBox fx:id="maleCheckBox" layoutX="676.0" layoutY="42.0" mnemonicParsing="false" onAction="#showMaleAction" text="Male" />
                </children></AnchorPane>
            <ScrollPane prefHeight="774.0" prefWidth="1214.0">
                <content>
                    <TilePane fx:id="allCardContent" prefHeight="758.0" prefWidth="1198.0" />
                </content>
            </ScrollPane>
        </children>
    </VBox>

</AnchorPane>
